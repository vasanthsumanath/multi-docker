{
 "AWSEBDockerrunVersion": 2,
 "containerDefinitions": [
   {
    "name": "client",
    "image": "vasanthsumanath/multi-client",
    "hostname": "client",
    "essential": false
   },
   {
    "name": "server",
    "image": "vasanthsumanath/multi-server",
    "hostname": "api",
    "essential": false
   }, 
   {
    "name": "worker",
    "image": "vasanthsumanath/multi-worker",
    "hostname": "worker",
    "essential": false
   },
   {
    "name": "nginx",
    "image": "vasanthsumanath/multi-nginx",
    "essential": true,
    "portMappings":[
       {
        "hostPort": 80,
        "containerPort": 80
       }
    ],
    "links":[
       "client",
       "server"
    ]
   }  
 ]
}
